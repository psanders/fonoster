# Fonoster
recordingsPath=<%= node[:fonoster][:recordingsPath] %>
ttsStorePath=<%= node[:fonoster][:ttsStorePath] %>
apiVersion=<%= node[:fonoster][:version] %>

# Credentials for admin APIs
admin.username=<%= node[:admin][:username] %>
admin.secret=<%= node[:admin][:secret] %>

# Asterisk
ast.manager.host=<%= node[:ast][:manager][:host] %>
ast.manager.port=<%= node[:ast][:manager][:port] %>
ast.manager.username=<%= node[:ast][:manager][:username] %>
ast.manager.secret=<%= node[:ast][:manager][:secret] %>
ast.dialplan.context=<%= node[:ast][:dialplan][:context] %>
ast.dialplan.ext=<%= node[:ast][:dialplan][:ext] %>

# Astive Server
astived.host=<%= node[:astived][:host] %>
astived.port=<%= node[:astived][:port] %>

# Mongo config
db.host=<%= node[:db][:host] %>
db.port=<%= node[:db][:port] %>
db.username=<%= node[:db][:username] %>
db.secret=<%= node[:db][:secret] %>
db.name=<%= node[:db][:name] %>

# BrainTree (Payment Gateway)
braintree.environment=<%= node[:braintree][:environment] %>
braintree.merchantId=<%= node[:braintree][:merchantId] %>
braintree.publicKey=<%= node[:braintree][:publicKey] %>
braintree.privateKey=<%= node[:braintree][:privateKey] %>

# Bluemix TTS Service Credentials
bluemix.username=<%= node[:bluemix][:username] %>
bluemix.password=<%= node[:bluemix][:password] %>

# Mail Gun config
mailgun.apiKey=<%= node[:mailgun][:apiKey] %>
mailgun.resource=<%= node[:mailgun][:resource] %>
mailgun.username=<%= node[:mailgun][:username] %>
mailgun.password=<%= node[:mailgun][:password] %>

email.admin=Fonoster Team <fonosterteam@fonoster.com>
email.team=Fonoster Team <fonosterteam@fonoster.com>
email.dev=Fonoster Team <fonosterteam@fonoster.com>