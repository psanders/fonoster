[general]
tcpenable=yes
udpbindaddr=0.0.0.0:6060
tcpbindaddr=0.0.0.0:6060
srvlookup=yes
context=<%= node[:ast][:dialplan][:context] %>
dtmfmode=inband
nat=force_rport,comedia

register => <%= node[:sip][:server][:username] %>:<%= node[:sip][:server][:secret] %>@sipio:<%= node[:sip][:server][:port] %>/<%= node[:sip][:server][:username] %>

[sipio]
type=peer
username=<%= node[:sip][:server][:username] %>
secret=<%= node[:sip][:server][:secret] %>
host=<%= node[:sip][:server][:host] %>

